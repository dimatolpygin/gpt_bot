# Telegram Bot
BOT_TOKEN=your_telegram_bot_token_here

# OpenAI
OPENAI_API_KEY=sk-your_openai_api_key_here
OPENAI_MODEL=gpt-4o

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_or_service_key_here

# Redis
REDIS_URL=redis://localhost:6379

# Whitelist (comma-separated Telegram user IDs). Empty = allow all.
ALLOWED_USERS=

# Proxy (if Telegram is blocked in your network)
# SOCKS5 example: socks5://user:pass@host:port
# HTTPS example:  https://user:pass@host:port
SOCKS5_PROXY=
HTTPS_PROXY=

# WebApp server
PORT=3000
# WEBAPP_URL — публичный адрес сервера, например ngrok/http://your-domain.com
WEBAPP_URL=https://your-domain.com
